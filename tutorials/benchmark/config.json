{
    "framework": "crypten",
    "dataset": "breast_horizontal",
    "algorithm": "mpc",
    "model": "logistic_regression",
    "bench_param": {
        "mode": "local",
        "device": "cpu"
    },
    "training_param": {
        "epochs": 3,
        "batch_size": 3,
        "learning_rate": 0.1,
        "loss_func": "cross_entropy",
        "optimizer": "sgd",
        "optimizer_param": {
            "momentum": 0.9,
            "dampening": 0,
            "weight_decay": 0,
            "nesterov": false
        }
    }
}
